  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "mrun": "yarn typeorm -d ./src/shared/infra/typeorm/index.ts migration:run",
    "mrevert": "yarn typeorm -d ./src/shared/infra/typeorm/index.ts migration:revert",
    "test": "jest"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
