    "@babel/preset-typescript": "^7.12.13",
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.8",
    "@types/node": "^20.1.0",
    "@types/node-cron": "^2.0.3",
    "@types/uuid": "^8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
