    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.8",
    "https": "^1.0.0",
    "node-cron": "^3.0.0",
    "path": "^0.12.7",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
